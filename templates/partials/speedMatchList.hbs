<div class="modal-wrap match">
	<div class="modal-inner match">

		<h3 class="message">{{message}}</h3>

		<div class="match-list">

		{{#each match}}

			<div class="match-inner">
				<img src="{{{cloudinaryUrl profiles.[0] width=200 height=200 quality=80}}}" />
				{{{rationale.html}}}
			</div>

		{{/each}}

		</div>

		<div class="button-group">

			<input id="back-btn" data-keep_enabled="true" class="submit" type="submit" name="submit" value="continue">

		</div>

	</div>
</div>
