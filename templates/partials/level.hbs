{{!-- {{{jsonPrint this}}} --}}
{{#ifeq level 'One'}}
	<section class="row level tinderslide" data-level="1" {{#if player.triesOne}}data-tries="{{player.triesOne}}" {{/if}}{{#if player.pointsOneCap}}data-max="{{player.pointsOneCap}}" {{/if}}>

		<ul style="height:100%;">
			{{#each (shuffle items)}}
				{{> item this}}
			{{/each}}
		</ul>

		{{!-- match modals --}}
		<div class="modal-wrapper"></div>

	</section>

{{/ifeq}}

